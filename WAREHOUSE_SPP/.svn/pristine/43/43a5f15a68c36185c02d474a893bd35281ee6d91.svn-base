<uicore:PopupBase x:Class="DSofT.Warehouse.UI.frm_NhapKho_popup"
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"  
             xmlns:uicore="clr-namespace:DSofT.Framework.UICore;assembly=DSofT.Framework.UICore"
             xmlns:resText="clr-namespace:DSofT.Warehouse.Resources;assembly=DSofT.Warehouse.Resources"
             xmlns:cntr="clr-namespace:DSofT.Framework.UIControl;assembly=DSofT.Framework.UIControl" 
             xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/navbar" 
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:Controls="clr-namespace:DSofT.Framework.Metro.UIControls;assembly=DSofT.Framework.Metro.UIControls" 
             Height="600" Width="1200" >
    <uicore:PopupBase.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/DSofT.Warehouse.Resources;component/CommonResource.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </uicore:PopupBase.Resources>
    <Grid Background="Transparent">
        <Grid.Resources>
            <cntr:ConverterStringDateTime x:Key="ConverterStringDateTime"/>
            <cntr:ConverterStringBool x:Key="ConverterStringBool"/>
        </Grid.Resources>
        <dxmvvm:Interaction.Behaviors>
            <dxmvvm:NotificationService x:Name="notificationService" ApplicationId="DxToastTest" UseWin8NotificationsIfAvailable="True" 
                                  PredefinedNotificationTemplate="{Binding ElementName=notificationStyle, Path=SelectedItem}"
                                  CustomNotificationTemplate="{StaticResource CustomNotificationTemplate}"
                                  Sound="Notification_Looping_Alarm8"
                                  CustomNotificationPosition="BottomRight" CustomNotificationVisibleMaxCount="5" CustomNotificationDuration="0:0:3"/>
        </dxmvvm:Interaction.Behaviors>
        <Grid>

            <Grid.RowDefinitions>
                <!--Button Area-->
                <RowDefinition Height="Auto"/>
                <!--Blank-->
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="0.1*"/>
            </Grid.RowDefinitions>
            <!--Selection Area Style : row=2-->
            <Border x:Name="BrdBtnWrap" Grid.Row="0" Grid.ColumnSpan="2" Style="{StaticResource Sdk.Brd.BtnArea}">
                <StackPanel Style="{StaticResource Sdk.Spnl.CommBtn.Right}">
                    <Button Tag="In mã vạch dán pallet" HorizontalAlignment="Right" Name="btnInMaVach" Style="{StaticResource Sdk.Btn.Comm}" VerticalAlignment="Center"  />
                    <Button Tag="Nhập hàng nhiều lần" HorizontalAlignment="Right" Name="btnNhapHangNhieuLan" Style="{StaticResource Sdk.Btn.Comm}" VerticalAlignment="Center"  />
                    <Button Tag="Thêm mới" HorizontalAlignment="Right" Name="btnThemmoi" Style="{StaticResource Sdk.Btn.Comm}" VerticalAlignment="Center" Click="btnThemmoi_Click"  />
                    <Button Tag="{StaticResource btnImportExcel}" HorizontalAlignment="Right" Name="btnImport" Style="{StaticResource Sdk.Btn.Comm}" VerticalAlignment="Center"  />
                    <Button Tag="{StaticResource btnSave}" HorizontalAlignment="Right"  Name="btnSave" Style="{StaticResource Sdk.Btn.Comm}"  VerticalAlignment="Center" Click="btnSave_Click" />
                </StackPanel>
            </Border>
            <Border x:Name="nbcSearch" Grid.Row="1" Grid.ColumnSpan="2" Style="{StaticResource Sdk.Brd.BtnArea}">
                <Grid x:Name="Selection_Area">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="25"/>
                        <RowDefinition Height="25"/>
                        <RowDefinition Height="25"/>
                        <RowDefinition Height="25"/>
                        <RowDefinition Height="25"/>
                        <RowDefinition Height="25"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="0.5*"/>
                    </Grid.ColumnDefinitions>


                    <!--begin block 1-->
                    <TextBlock Grid.Column="0" Grid.Row="0" Name="lblNCC"  Text="NCC/KH"/>
                    <dxe:ComboBoxEdit IsTextEditable="True" ValidateOnTextInput="False" EditValue="{Binding KHACHHANG_NCC_ID,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" IncrementalFiltering="True" ImmediatePopup="True" FilterCondition = "Contains" Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="5"  Name="cboNCC" EditValueChanged="cboNCC_EditValueChanged"  />

                    <TextBlock Grid.Column="0" Grid.Row="1" Name="lbldiadiemxuathang"  Text="Địa điểm xuất hàng"/>
                    <dxe:ComboBoxEdit Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="5"  Name="cboDiadiemxuathang"   IsTextEditable="True" ValidateOnTextInput="False" EditValue="{Binding KHACHHANG_NCC_NOI_XUATHANG_ID,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" IncrementalFiltering="True" ImmediatePopup="True" FilterCondition = "Contains"/>

                    <TextBlock Grid.Column="0" Grid.Row="2" Name="lblnguoilienhe"  Text="Người liên hệ"/>
                    <dxe:TextEdit Grid.Row="2" Grid.Column="1"  Grid.ColumnSpan="3"  ValidateOnTextInput="False"  EditValue="{Binding NGUOILIENHE_A}"  Name="txtNGUOILIENHE_A" />


                    <TextBlock Grid.Column="0" Grid.Row="3" Name="lblsochungtu"  Text="Số chứng từ"/>
                    <dxe:TextEdit Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="3"    Name="txtSO_CHUNGTU"   ValidateOnTextInput="False"  EditValue="{Binding SO_CHUNGTU}"  />

                    <TextBlock Grid.Column="4" Grid.Row="3" Name="lblngaychungtu"  Text="Ngày CT"/>
                    <dxe:DateEdit Grid.Column="5" Grid.Row="3" MaskType="DateTime" Name="txtNGAY_CHUNGTU" MaskUseAsDisplayFormat="True" EditValue="{Binding NGAY_CHUNGTU, Converter={StaticResource ConverterStringDateTime}, ConverterParameter='dd/MM/yyyy', Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Mask="dd/MM/yyyy"  ShowClearButton="False" EditValueChanged="txtNGAY_CHUNGTU_EditValueChanged" />
                   

                    <TextBlock Grid.Column="4" Grid.Row="2" Name="lblIDPHIEU"  Text="ID Phiếu"/>
                    <dxe:TextEdit Grid.Row="2" Grid.Column="5"   Name="txtPHIEUYEUCAU_NHAPKHO_ID"  IsEnabled="False"  ValidateOnTextInput="False"  EditValue="{Binding PHIEU_NHAPXUATKHO_ID}"/>
                    <!--end block 1-->


                    <!--begin block 2-->
                    <TextBlock Grid.Column="6" Grid.Row="0" Name="lblkhonhap"  Text="Kho nhập"/>
                    <dxe:TextEdit Grid.Column="7" Grid.Row="0" Grid.ColumnSpan="3" IsEnabled="False"  EditValue="{Binding TEN_KHO}"   Name="txtkhonhap"/>

                    <TextBlock Grid.Column="6" Grid.Row="1" Name="lblnguoilienhe1"  Text="Người liên hệ"/>
                    <dxe:TextEdit Grid.Column="7" Grid.Row="1" Grid.ColumnSpan="3"  ValidateOnTextInput="False"  EditValue="{Binding NGUOILIENHE_B}"  Name="txtNGUOILIENHE_B"/>

                    <TextBlock Grid.Column="6" Grid.Row="2" Name="lblsophieu"  Text="Số phiếu">
                         <Run Text="{StaticResource lblDauSao}" Foreground="Red" />
                    </TextBlock>

                    <dxe:TextEdit Grid.Column="7" Grid.Row="2" ValidateOnTextInput="False"  EditValue="{Binding SOPHIEU}"   Name="txtSOPHIEU"/>


                    <TextBlock Grid.Column="8" Grid.Row="2" Name="lblngaynhap"  Text="Ngày nhập">
                         <Run Text="{StaticResource lblDauSao}" Foreground="Red" />
                    </TextBlock>
                    <dxe:DateEdit Grid.Column="9" Grid.Row="2" MaskType="DateTime" Name="txtNGAYLAP" MaskUseAsDisplayFormat="True" EditValue="{Binding NGAYLAP, Converter={StaticResource ConverterStringDateTime}, ConverterParameter='dd/MM/yyyy', Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Mask="dd/MM/yyyy"  ShowClearButton="False" MaskAutoComplete="Strong" MaskSaveLiteral="False" EditValueChanged="txtNGAYLAP_EditValueChanged"/>

                    <TextBlock Grid.Column="6" Grid.Row="3" Name="lblhinhthucnhap"  Text="Hình thức nhập">
                        <Run Text="{StaticResource lblDauSao}" Foreground="Red" />
                    </TextBlock>
                    <dxe:ComboBoxEdit Grid.Column="7" Grid.Row="3"  Name="cbohinhthucnhap" Grid.ColumnSpan="3"   IsTextEditable="True" ValidateOnTextInput="False" EditValue="{Binding MA_HINHTHU_NHAPXUAT,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" IncrementalFiltering="True" ImmediatePopup="True" FilterCondition = "Contains"/>
                    <!--end block 2-->



                    <!--begin block 3-->
                    <TextBlock Grid.Column="10" Grid.Row="0" Name="lbldonvivanchuyen"  Text="Đơn vị vận chuyển"/>
                    <dxe:ComboBoxEdit Grid.Column="11" Grid.Row="0"  Name="cboDonvivanchuyen" Grid.ColumnSpan="3"  IsTextEditable="True" ValidateOnTextInput="False" EditValue="{Binding KHACHHANG_NCC_DONVI_VANCHUYEN_ID,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" IncrementalFiltering="True" ImmediatePopup="True" FilterCondition = "Contains"/>

                    <TextBlock Grid.Column="10" Grid.Row="1" Name="lbltaixe"  Text="Tài xế"/>
                    <dxe:TextEdit Grid.Column="11" Grid.Row="1"  Grid.ColumnSpan="3" ValidateOnTextInput="False"  EditValue="{Binding TEN_TAIXE}"   Name="txtTEN_TAIXE"/>

                    <TextBlock Grid.Column="10" Grid.Row="2" Name="lblsodienthoai"  Text="Số điện thoại"/>
                    <dxe:TextEdit Grid.Column="11" Grid.Row="2"  Grid.ColumnSpan="3"   MaskType="RegEx" Mask="((\d{3}))-(\d{3})-(\d{4}) (-(\d*))?" ValidateOnTextInput="False"  EditValue="{Binding SO_DIENTHOAI}"  Name="txtSO_DIENTHOAI"/>

                    <TextBlock Grid.Column="10" Grid.Row="3" Name="lblsoxe"  Text="Số xe"/>
                    <dxe:TextEdit Grid.Column="11" Grid.Row="3" ValidateOnTextInput="False"  EditValue="{Binding SO_XE}"   Name="txtSO_XE"/>

                    <TextBlock Grid.Column="12" Grid.Row="3" Name="lblsocont"  Text="Số cont"/>
                    <dxe:TextEdit Grid.Column="13" Grid.Row="3" ValidateOnTextInput="False"  EditValue="{Binding SO_CONT}"   Name="txtSO_CONT"/>

                    <TextBlock Grid.Column="12" Grid.Row="4" Name="lblPHIEU_KIEM_KE"  Text="Số phiếu yêu cầu"/>
                    <dxe:ComboBoxEdit Grid.Column="13" Grid.Row="4"  Name="cboPHIEU_KIEM_KE" Grid.ColumnSpan="3" Visibility="Hidden" IsTextEditable="True" ValidateOnTextInput="False" EditValue="{Binding PHIEU_KIEMKE_ID,UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" IncrementalFiltering="True" ImmediatePopup="True" FilterCondition = "Contains"/>
                    <dxe:TextEdit Grid.Column="13" Grid.Row="4" ValidateOnTextInput="False"  IsEnabled="False"   Name="txtID_PHIEU_YC"/>
                    
                    <TextBlock Grid.Column="0" Grid.Row="4" Name="lblghichu"  Text="Ghi chú"/>
                    <dxe:TextEdit Grid.Column="1" Grid.Row="4" Grid.ColumnSpan="9"  ValidateOnTextInput="False"  EditValue="{Binding GHI_CHU}"  Name="txtGHI_CHU"/>
                    <CheckBox Margin="5 0 0 0" Grid.Column="10" Grid.ColumnSpan="2" Grid.Row="4" Name="chkIS_NHAPNHIEULAN" Content="Nhập nhiều lần" FlowDirection="RightToLeft" HorizontalAlignment="Left" Click="chkIS_NHAPNHIEULAN_Click"/>

                    <Button Tag="Chọn sản phẩm bổ sung"  Grid.Column="0" Grid.ColumnSpan="2"   Grid.Row="5"  Name="btnChonSPBSung" Style="{StaticResource Sdk.Btn.Comm}"  VerticalAlignment="Center" Click="btnChonSPBSung_Click"/>
                    <Button Tag="Chọn phiếu yêu cầu"  Grid.Column="3" Grid.ColumnSpan="2"   Grid.Row="5"  Name="btnChonPhieuYC" Style="{StaticResource Sdk.Btn.Comm}"  VerticalAlignment="Center" Click="btnChonPhieuYC_Click"/>
                    <Button Tag="Đã kiểm tra đơn hàng"  Grid.Column="11"  Grid.ColumnSpan="2"  Grid.Row="5"  Name="btnKiemTraDonHang" Style="{StaticResource Sdk.Btn.Comm}" VerticalAlignment="Center"/>
                    <Button Tag="Xóa dòng"  Grid.Column="13"   Grid.Row="5"  Name="btnXoadong" Style="{StaticResource Sdk.Btn.Comm}"  VerticalAlignment="Center" Click="btnXoadong_Click"/>

                    <!--end block 3-->


                </Grid>
            </Border>
            <Border x:Name="nbcgrid" Grid.Row="2" Grid.ColumnSpan="2" Style="{StaticResource Sdk.Brd.BtnArea}">
                <Grid Grid.Row="2" x:Name="Body_Area" Grid.Column="0" Style="{StaticResource Sdk.Grd.BodyWrap.Vessel}">
                    <dxg:GridControl AutoPopulateColumns="True" x:Name="grd"   Grid.Column="0"  >
                        <dxg:GridControl.View>
                            <dxg:TableView AutoWidth="False"   NavigationStyle="Cell" ShowGroupPanel="False"  SearchPanelFindFilter="Contains"   UseAnimationWhenExpanding="False"
                                          x:Name="grdView" CellValueChanged="grdView_CellValueChanged"  />
                        </dxg:GridControl.View>
                    </dxg:GridControl>
                </Grid>
            </Border>
        </Grid>
    </Grid>
</uicore:PopupBase>