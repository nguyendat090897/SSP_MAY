    public static long NVL_NUM_LONG_NEW(object str)
    {
        if ((str != System.DBNull.Value) && (str != null))
        {
            if (str.ToString().Trim().Equals("") == false)
            {
                return long.Parse(str.ToString());
            }
            else
            {
                return 0;
            }
        }
        else
        {
            return 0;
        }
    }

