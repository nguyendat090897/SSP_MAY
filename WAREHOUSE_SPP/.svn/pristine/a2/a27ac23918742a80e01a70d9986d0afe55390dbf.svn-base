<uicore:PopupBase x:Class="DSofT.Warehouse.UI.frm_ThuocDieuTri_Popup"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"  
             xmlns:uicore="clr-namespace:DSofT.Framework.UICore;assembly=DSofT.Framework.UICore"
             xmlns:resText="clr-namespace:DSofT.Warehouse.Resources;assembly=DSofT.Warehouse.Resources"
             xmlns:cntr="clr-namespace:DSofT.Framework.UIControl;assembly=DSofT.Framework.UIControl" 
             xmlns:dxn="http://schemas.devexpress.com/winfx/2008/xaml/navbar" 
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:Controls="clr-namespace:DSofT.Framework.Metro.UIControls;assembly=DSofT.Framework.Metro.UIControls" 
             Height="500" Width="800" >
    <uicore:PopupBase.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/DSofT.Warehouse.Resources;component/CommonResource.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </uicore:PopupBase.Resources>
    <Grid Background="Transparent">
        <Grid.Resources>
            <cntr:ConverterStringDateTime x:Key="ConverterStringDateTime"/>
        </Grid.Resources>
        <dxmvvm:Interaction.Behaviors>
            <dxmvvm:NotificationService x:Name="notificationService" ApplicationId="DxToastTest" UseWin8NotificationsIfAvailable="True" 
                                  PredefinedNotificationTemplate="{Binding ElementName=notificationStyle, Path=SelectedItem}"
                                  CustomNotificationTemplate="{StaticResource CustomNotificationTemplate}"
                                  Sound="Notification_Looping_Alarm8"
                                  CustomNotificationPosition="BottomRight" CustomNotificationVisibleMaxCount="5" CustomNotificationDuration="0:0:3"/>
        </dxmvvm:Interaction.Behaviors>
        <Grid>
            <!--Main Layout RowDefinition-->
            <Grid.RowDefinitions>
                <!--Button Area-->
                <RowDefinition Height="auto"/>
                <!--Blank-->
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <!--Selection Area Style : row=2-->
            <Border x:Name="BrdBtnWrap" Grid.Row="0" Grid.ColumnSpan="2" Style="{StaticResource Sdk.Brd.BtnArea}">
                <StackPanel Style="{StaticResource Sdk.Spnl.CommBtn.Right}">
                    <Button Tag="{StaticResource btnSave}" HorizontalAlignment="Right" Name="btnSave" Style="{StaticResource Sdk.Btn.Comm}" VerticalAlignment="Center" />
                    <Button Tag="{StaticResource btnClose}" HorizontalAlignment="Right" Name="btnClose" Style="{StaticResource Sdk.Btn.Comm}" VerticalAlignment="Center" Click="btnClose_Click"/>
                </StackPanel>
            </Border>
            <dxn:NavBarControl Grid.Row="1" Grid.Column="1" Name="nbcSearch" Style="{StaticResource Dx.NavBar.CommSchWrap}" Margin="6.85">
                <dxn:NavBarControl.View>
                    <dxn:ExplorerBarView 
                        Style="{StaticResource Dx.NavBar.Explorer.CommSch}" />
                </dxn:NavBarControl.View>
                <dxn:NavBarGroup x:Name="cbgSearch" >
                    <dxn:NavBarItem x:Name="cbiSearch">
                        <Grid x:Name="Selection_Area">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="25"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="0.5*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="0.5*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="0.5*"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock  Grid.Row="0"   Grid.Column="0" Name="lblMaThuoc" VerticalAlignment="Center"><Run Text="{DynamicResource lblMaThuoc}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="0" Grid.ColumnSpan="2" Grid.Column="1" Name="txtmathuoc" Text="{}"/>
                            <TextBlock  Grid.Row="0"   Grid.Column="3" Name="lblMaPhu" VerticalAlignment="Center"><Run Text="{DynamicResource lblMaPhu}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="0" Grid.Column="4" Grid.ColumnSpan="2" Name="txtMaPhu" Text="{}"/>

                            <TextBlock Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="5" Name="lblChuThich" VerticalAlignment="Center" HorizontalAlignment="Right" Foreground="Red"><Run Text="{DynamicResource lblChuThich}" /></TextBlock>
                            
                            <TextBlock  Grid.Row="2"   Grid.Column="0" Name="lblTenThuoc" VerticalAlignment="Center"><Run Text="{DynamicResource lblTenThuoc}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="5" Name="txtTenThuoc" Text="{}"/>

                            <TextBlock  Grid.Row="3"   Grid.Column="0" Name="lblMaVach" VerticalAlignment="Center"><Run Text="{DynamicResource lblMaVach}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="2" Name="txtMaVach" Text="{}" MaskType="Numeric" Mask="#,##,##,##,##,##,##,##,##,##,##,##,##,###" MaskUseAsDisplayFormat="True" />
                            <TextBlock  Grid.Row="3"   Grid.Column="3" Name="lblThuocKSBD" VerticalAlignment="Center"><Run Text="{DynamicResource lblThuocKSDB}"/></TextBlock>
                            <dxe:CheckEdit Grid.Row="3" Grid.Column="4" Name="chkThuocKSBD" />

                            <TextBlock  Grid.Row="4"   Grid.Column="0" Name="lblNhomSP" VerticalAlignment="Center"><Run Text="{DynamicResource lblNhomSP}"/></TextBlock>
                            <dxe:ComboBoxEdit Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2"  Name="cboNhomSP" Text="{}"/>
                            <TextBlock  Grid.Row="4"   Grid.Column="3" Name="lblHoatChatChinh" VerticalAlignment="Center"><Run Text="{DynamicResource lblHoatChatChinh}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="4" Grid.Column="4" Grid.ColumnSpan="2" Name="txtHoatChatChinh"  Text="{}"/>

                            <TextBlock  Grid.Row="5"   Grid.Column="0" Name="lblDVT" VerticalAlignment="Center"><Run Text="{DynamicResource lblDVT}"/></TextBlock>
                            <dxe:ComboBoxEdit Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="2"  Name="cboDVT" Text="{}"/>
                            <TextBlock  Grid.Row="5"   Grid.Column="3" Name="lblTonKho" VerticalAlignment="Center"><Run Text="{DynamicResource lblTonKho}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="5" Grid.Column="4" Grid.ColumnSpan="2" Name="txtTonKho" MaskType="Numeric" Mask="#,##,##,##,##,##,##,##,##,##,##,##,##,###" MaskUseAsDisplayFormat="True"   Text="{}"/>

                            <TextBlock  Grid.Row="6"   Grid.Column="0" Name="lblTuThuoc" VerticalAlignment="Center"><Run Text="{DynamicResource lblTuThuoc}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="2" Name="txtTuThuoc"  Text="{}"/>
                            <TextBlock  Grid.Row="6"   Grid.Column="3" Name="lblDKBQ" VerticalAlignment="Center"><Run Text="{DynamicResource lblDKBQ}"/></TextBlock>
                            <dxe:ComboBoxEdit Grid.Row="6" Grid.Column="4" Grid.ColumnSpan="2"  Name="cboDKBQ" Text="{}"/>
                            
                            <TextBlock  Grid.Row="7"   Grid.Column="0" Name="lblGiaBanLe" VerticalAlignment="Center"><Run Text="{DynamicResource lblGiaBanLe}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="2" Name="txtGiaBanLe" MaskType="Numeric" Mask="#,##,##,##,##,##,##,##,##,##,##,##,##,###" MaskUseAsDisplayFormat="True"   Text="{}"/>
                            <TextBlock  Grid.Row="7"   Grid.Column="3" Name="lblGiaBanLuon" VerticalAlignment="Center"><Run Text="{DynamicResource lblGiaBanLuon}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="7" Grid.Column="4" Grid.ColumnSpan="2" Name="txtGiaBanLuon" MaskType="Numeric" Mask="#,##,##,##,##,##,##,##,##,##,##,##,##,###" MaskUseAsDisplayFormat="True"   Text="{}"/>
                            
                            <TextBlock  Grid.Row="8"   Grid.Column="0" Name="lblTrongLuong" VerticalAlignment="Center"><Run Text="{DynamicResource lblTrongLuong}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="8" Grid.Column="1" Grid.ColumnSpan="5" Name="txtTrongLuong"  Text="{}"/>

                            <TextBlock  Grid.Row="9"   Grid.Column="0" Name="lblNhaSX" VerticalAlignment="Center"><Run Text="{DynamicResource lblNhaSX}"/></TextBlock>
                            <dxe:ComboBoxEdit Grid.Row="9" Grid.Column="1" Grid.ColumnSpan="2" Name="cbbNhaSX"  Text="{}"/>
                            <TextBlock  Grid.Row="9"   Grid.Column="3" Name="lblNuocSX" VerticalAlignment="Center"><Run Text="{DynamicResource lblNuocSX}"/></TextBlock>
                            <dxe:ComboBoxEdit Grid.Row="9" Grid.Column="4" Grid.ColumnSpan="2"  Name="cbbNuocSX" Text="{}"/>

                            <TextBlock  Grid.Row="10"   Grid.Column="0" Name="lblNhaCC" VerticalAlignment="Center"><Run Text="{DynamicResource lblNhaCC}"/></TextBlock>
                            <dxe:ComboBoxEdit Grid.Row="10" Grid.Column="1" Grid.ColumnSpan="5"  Name="cboNhaCC" Text="{}"/>

                            <TextBlock  Grid.Row="11"   Grid.Column="0" Name="lblThue" VerticalAlignment="Center"><Run Text="{DynamicResource lblThue}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="11" Grid.Column="1" Name="txtThue" MaskType="Numeric" Mask="#,##,##,##,##,##,##,##,##,##,##,##,##,###" MaskUseAsDisplayFormat="True"  Text="{}"/>
                            <TextBlock  Grid.Row="11"   Grid.Column="2" Name="lblChietKhau" VerticalAlignment="Center"><Run Text="{DynamicResource lblChietKhau}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="11" Grid.Column="3" Name="txtChietKhau" MaskType="Numeric" Mask="#,##,##,##,##,##,##,##,##,##,##,##,##,###" MaskUseAsDisplayFormat="True"  Text="{}"/>
                            <TextBlock  Grid.Row="11"   Grid.Column="4" Name="lblThuongDS" VerticalAlignment="Center"><Run Text="{DynamicResource lblThuong}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="11" Grid.Column="5" Name="txtThuongDS" MaskType="Numeric" Mask="#,##,##,##,##,##,##,##,##,##,##,##,##,###" MaskUseAsDisplayFormat="True"   Text="{}"/>

                            <TextBlock Grid.Row="12" Grid.Column="0" Name="lblAnh" VerticalAlignment="Center"><Run Text="{DynamicResource lblAnh}" /></TextBlock>
                            <dxe:TextEdit Grid.Row="12" Grid.Column="1" Grid.ColumnSpan="3" Name="txtAnh" Text="{}"></dxe:TextEdit>
                            <Button Grid.Row="12" Grid.Column="4" Width="0.5" Height="1" Name="btnAnh" Tag="{StaticResource btnBrowser}" Style="{StaticResource Sdk.Btn.Comm}" Click="btnAnh_Click"/>

                            <TextBlock  Grid.Row="13"   Grid.Column="0" Name="lblConSuDung" VerticalAlignment="Center"><Run Text="{DynamicResource lblConSD}"/></TextBlock>
                            <RadioButton Grid.Row="13" Grid.Column="1" Name="rdbDangSD" Content="{DynamicResource lblDangSD}" IsChecked="True" />
                            <RadioButton Grid.Row="13" Grid.Column="2" Name="rdbKhongSD" Content="{DynamicResource lblKhongSD}" />
                            
                            <TextBlock  Grid.Row="14"   Grid.Column="0" Name="lblQuyCach" VerticalAlignment="Center"><Run Text="{DynamicResource lblQuyCach}"/></TextBlock>
                            <dxe:TextEdit Grid.Row="14" Grid.Column="1" Grid.ColumnSpan="4" Name="txtQuyCach"  Text="{}"/>

                            <Image Grid.Row="12" Grid.Column="5" Grid.RowSpan="3" Name="imgAnh" />

                        </Grid>
                    </dxn:NavBarItem>
                </dxn:NavBarGroup>
            </dxn:NavBarControl>

        </Grid>
    </Grid>
</uicore:PopupBase>

